<?xml version="1.0" encoding="UTF-8"?>
<view name="po-exception-view" template="po-exception-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<labels default="en">
		<locale name="en">
			<label name="Reject" value="Workflow Terminate"/>
			<label name="forward" value="Complete"/>
		</locale>
	</labels>

	<section-overrides>	

	</section-overrides>

	<property-overrides>

	</property-overrides>

	<lineItem-overrides>
			 
	</lineItem-overrides>

	<action-overrides>
		
	</action-overrides>

	<hidden-overrides>
	<hidden name="processInformation.response" defaultValue="Complete"/>	
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Rescan" commentsRequired="false" note="message.rescan">
			</reason>
			<reason name="Vendor Maintenance" commentsRequired="false" note="message.returnVendorMaintenance">
			</reason>
			<reason name="Reject" commentsRequired="false" note="message.terminate">
			</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.duplicateInvoiceCheck" messageType="warning" note="message.duplicateInvoiceValidation"/>
		<validation name="validation.validVendor" note="message.validVendorValidation"/>
		<!--validation name="validation.lineItemAmount"/-->
	</validations>
</view>